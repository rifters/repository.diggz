<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.diggzwizard" name="Diggz Fire Builds Wizard" version="2.3" provider-name="Jason Diggz">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Diggz Fire Builds Wizard</summary>
    <description lang="en">Install, update, and get help with Diggz Fire Builds</description>
    <platform>all</platform>
  </extension>
</addon>